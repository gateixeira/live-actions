      {
        "id": 8,
        "title": "Average Queue Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(github_runners_queue_duration_seconds_sum[$__rate_interval]) / rate(github_runners_queue_duration_seconds_count[$__rate_interval])) or vector(0)",
